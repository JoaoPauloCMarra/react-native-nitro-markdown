{
  "name": "react-native-nitro-markdown-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "node scripts/setup.js",
    "publish-package": "node scripts/publish.js",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "codegen": "turbo run codegen",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "test:cpp": "turbo run test:cpp --filter=react-native-nitro-markdown",
    "benchmark": "node scripts/benchmark-comparison.js",
    "start": "turbo run start --filter=example --",
    "example:ios": "turbo run ios --filter=example",
    "example:android": "turbo run android --filter=example",
    "example:prebuild": "turbo run prebuild --filter=example",
    "example:prebuild:clean": "turbo run prebuild:clean --filter=example"
  },
  "devDependencies": {
    "@types/commonmark": "^0.27.10",
    "@types/jest": "^29.5.14",
    "@types/markdown-it": "^14.1.2",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.19.33",
    "@types/react": "^19.2.14",
    "@types/react-native": "^0.73.0",
    "commonmark": "^0.31.2",
    "eslint-config-expo-magic": "^2.0.2",
    "jest": "^29.7.0",
    "markdown-it": "^14.1.1",
    "marked": "^17.0.2",
    "nitrogen": "^0.33.9",
    "react-native-builder-bob": "^0.40.18",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "react-native-nitro-modules": "0.33.9"
  },
  "dependencies": {
    "react-native-nitro-modules": "^0.33.9"
  },
  "packageManager": "bun@1.1.38"
}
